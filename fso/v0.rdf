<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
  xmlns:dc="http://purl.org/dc/elements/1.1/"
  xmlns:dcterms="http://purl.org/dc/terms/"
  xmlns:foaf="http://xmlns.com/foaf/0.1/"
  xmlns:owl="http://www.w3.org/2002/07/owl#"	
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xmlns:muto="http://purl.org/muto/core#">
  <!-- /// OWL DL compliance statements /// -->
  <!-- Annotation properties -->
  <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
  <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
  <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/licence"/>
  <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
  <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/issued"/>
  <owl:AnnotationProperty rdf:about="http://xmlns.com/foaf/0.1/homepage"/>
  <owl:AnnotationProperty rdf:about="http://xmlns.com/foaf/0.1/depiction"/>
  <!-- Classes -->
  <owl:Class rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>
  <owl:Class rdf:about="http://rdfs.org/sioc/ns#Item"/>
  <owl:Class rdf:about="http://rdfs.org/sioc/ns#UserAccount"/>
  <!-- Object properties -->
  <owl:ObjectProperty rdf:about="http://rdfs.org/sioc/ns#about"/>
  <owl:ObjectProperty rdf:about="http://rdfs.org/sioc/ns#has_creator"/>
  <owl:ObjectProperty rdf:about="http://rdfs.org/sioc/ns#creator_of"/>
  <!-- Datatype properties -->
  <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/created"/>
  <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/modified"/>
  <!-- /// MUTO Core ontology /// -->
  <owl:Ontology rdf:about="http://localhost:8000/fso">
    <dc:title>Freesound Ontology (FSO)</dc:title>
    <rdfs:label xml:lang="en">FSO Core Ontology</rdfs:label>
    <dc:description>The Freesound Ontology is defined to extend the semantic capabilities of the MUTO Ontology in the Freesound context</dc:description>
    <dc:creator>Sergio Oramas, Frededric Font</dc:creator>
    <dcterms:issued>2013-12-16</dcterms:issued>
    <dcterms:licence>http://creativecommons.org/licenses/by/3.0/</dcterms:licence>
    <rdfs:seeAlso>http://purl.org/muto/fso#</rdfs:seeAlso>
    <foaf:homepage>http://purl.org/fso</foaf:homepage>
    <foaf:depiction>http://purl.org/fso/core/muto-compact.png</foaf:depiction>
    <foaf:depiction>http://purl.org/fso/core/muto-UML.png</foaf:depiction>
    <owl:versionInfo>Version 0.1</owl:versionInfo>
  </owl:Ontology>
  <!-- Classes -->
  <owl:Class rdf:about="http://localhost:8000/fso#InstrumentTag">
    <rdfs:label xml:lang="en">InstrumentTag</rdfs:label>
    <rdfs:comment xml:lang="en">Instrument Tag</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/muto/core#Tag"/>
	<rdfs:isDefinedBy rdf:resource="http://localhost:8000/fso#"/>
  </owl:Class>
  <owl:Class rdf:about="http://localhost:8000/fso#SourceTag">
    <rdfs:label xml:lang="en">SourceTag</rdfs:label>
    <rdfs:comment xml:lang="en">Source Tag</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/muto/core#Tag"/>
